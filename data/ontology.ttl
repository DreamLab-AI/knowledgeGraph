    dt:authorityScore 0.95 ;
    dt:maturity "established" .

ai:TextGeneration a owl:Class ;
    rdfs:label "Text Generation"@en ;
    dcterms:identifier "AI-0374" ;
    rdfs:comment "Text Generation is the NLP task of producing coherent, contextually appropriate natural language text using neural language models, including applications such as story generation, article writing, code generation, and creative content production. Modern text generation employs transformer-based language models (GPT, T5, BLOOM) with autoregressive or sequence-to-sequence architectures, controllable generation techniques, and prompt engineering to produce human-quality text across diverse domains and styles."@en .

ai:TextSummarisation a owl:Class ;
